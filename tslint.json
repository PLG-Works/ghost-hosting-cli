{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "compilerOptions": {
    "lib": ["es6"],
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "rules": {
    "no-console": false,
    "no-unused-expression": false,
    "no-var-requires": false,
    "one-variable-per-declaration": false
  }
}
